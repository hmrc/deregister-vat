# microservice specific routes

DELETE      /data/:vrn             controllers.DataController.removeAll(vrn: String)

PUT         /data/:vrn/:key        controllers.DataController.storeData(vrn: String, key: String)
GET         /data/:vrn/:key        controllers.DataController.getData(vrn: String, key: String)
DELETE      /data/:vrn/:key        controllers.DataController.removeData(vrn: String, key: String)
